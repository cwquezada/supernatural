        <div class="contact is-form" ng-show="interface.isForm" ng-cloak>
          <form class="contact-form" name="contactForm" novalidate>
            <p class="control has-icon has-icon-right">
              <input class="input" ng-class="{'is-danger': contactForm.name.$invalid && !contactForm.name.$pristine}" type="text" name="name" ng-model="name" placeholder="Name" required>
              <span ng-show="contactForm.name.$invalid && !contactForm.name.$pristine" class="icon is-small">
                <i class="fa fa-warning"></i>
              </span>
              <span ng-show="contactForm.name.$invalid && !contactForm.name.$pristine" class="help is-danger">
                Please provide your name.
              </span>
            </p>
            <p class="control has-icon has-icon-right">
              <input class="input" ng-class="{'is-danger': contactForm.email.$invalid && !contactForm.email.$pristine}" type="email" name="email" ng-model="email" placeholder="Email" required>
              <span ng-show="contactForm.email.$invalid && !contactForm.email.$pristine" class="icon is-small">
                <i class="fa fa-warning"></i>
              </span>
              <span ng-show="contactForm.email.$invalid && !contactForm.email.$pristine" class="help is-danger">
                Please provide a valid email.
              </span>
            </p>
            <p class="control has-icon has-icon-right honeyPot">
              <input class="input" type="text" placeholder="Please Don't Fill In" ng-model="honeyPot" name="honeyPot">
              <span class="icon is-small">
                <i class="fa fa-warning"></i>
              </span>
            </p>
            <p class="control">
              <textarea class="textarea" placeholder="Message" name="message" ng-model="message"></textarea>
            </p>
            <div class="control is-grouped">
              <p class="control">
                <button ng-show="!interface.isLoading" class="button is-send" ng-class="{'is-disabled': contactForm.$invalid}" ng-click="submitForm(contactForm.$valid)">Send</button>
                <a ng-show="interface.isLoading" class="button is-loading">Loading</a>
              </p>
              <p class="control">
                <button class="button" ng-click="closeContact()">Cancel</button>
              </p>
            </div>
          </form>
        </div>

        <div class="contact" ng-show="interface.isRecieved" ng-cloak>
          <h3 class="title is-4">
            Thank you for your message.<br>We will consult the stars and get back to you soon.
          </h3>
          <p class="has-text-centered">
            <button class="button is-black" ng-click="closeContact()">Close</button>
          </p>
        </div>

        <div class="contact" ng-show="interface.isLost" ng-cloak>
          <h3 class="title is-4">
            Something happened in the universe that is beyond our control. Please come back soon and try again.
          </h3>
          <p class="has-text-centered">
            <button class="button is-black" ng-click="closeContact()">Close</button>
          </p>
        </div>
        
      </div>

      <button class="modal-close" ng-click="closeContact()"></button>

    </div>
    
    <%- body %>

